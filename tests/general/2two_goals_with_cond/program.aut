# Simple test with two goals and a conditional.
import general
labels

	go: [
		  predicate: entity_in,
		  params: [sauron, kitchen]
		 ],
	approach: [
				 predicate: agent_near,
				 params: [sauron, david]
	           ],
	someone_home: [
					predicate: entity_in,
					params: [david, kitchen]
	              ]


endlabels

module

	st: [0: init, 1: go, 2: approach];
	guard: [0: someone_home];

	[] 0 -> 1;
	[] 1 & guard=0 -> 2;

endmodule

options

	conditional_effects;

endoptions